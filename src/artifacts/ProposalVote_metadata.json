		"devdoc": {
			"details": "Quản lý biểu quyết chi tiêu. Yêu cầu 2/3 phiếu chấp thuận.",
			"events": {
				"ProposalCreated(uint256,address,uint256,string,address)": {
					"details": "Sự kiện khi tạo đề xuất mới"
				},
				"ProposalExecuted(uint256,address,uint256)": {
					"details": "Sự kiện khi một đề xuất được thực thi thành công"
				},
				"Voted(uint256,address,bool)": {
					"details": "Sự kiện khi có thành viên biểu quyết"
				}
			},
			"kind": "dev",
			"methods": {
				"createProposal(address,uint256,string)": {
					"details": "Tạo một đề xuất chi tiêu mới",
					"params": {
						"_amount": "Số tiền muốn chi",
						"_reason": "Lý do chi tiêu",
						"_recipient": "Địa chỉ ví nhận tiền"
					}
				},
				"getApprovalThreshold()": {
					"details": "Tính số phiếu cần thiết để phê duyệt đề xuất"
				},
				"getBalance()": {
					"details": "Kiểm tra số dư hiện tại của DAO"
				},
				"getProposal(uint256)": {
					"details": "Lấy thông tin chi tiết của một đề xuất",
					"params": {
						"_proposalId": "ID của đề xuất"
					}
				},
				"proposeAddMember(address)": {
					"details": "Yêu cầu thêm một thành viên mới thông qua biểu quyết",
					"params": {
						"_newMember": "Địa chỉ của thành viên mới"
					}
				},
				"proposeRemoveMember(address)": {
					"details": "Yêu cầu loại bỏ một thành viên thông qua biểu quyết",
					"params": {
						"_member": "Địa chỉ của thành viên bị loại bỏ"
					}
				},
				"voteProposal(uint256,bool)": {
					"details": "Biểu quyết một đề xuất",
					"params": {
						"_approve": "Đồng ý hay từ chối (true = đồng ý, false = từ chối)",
						"_proposalId": "ID của đề xuất"
					}
				},
				"voteToAddMember(uint256,bool)": {
					"details": "Biểu quyết thêm thành viên mới",
					"params": {
						"_approve": "Đồng ý hay từ chối (true = đồng ý, false = từ chối)",
